{
  "package": {
    "productName": "blob-downloader",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../www",
    "devPath": "http://localhost:3333/",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": "npm run build",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "com.cheesegrinder.blob-downloader",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "label": "main",
        "title": "blob-downloader",
        "width": 1200,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
