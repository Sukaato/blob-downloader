{
  "package": {
    "productName": "blob-downloader",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../www",
    "devPath": "http://localhost:3333/",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": "npm run build",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "com.cheesegrinder.blob-downloader",
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": ""
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$APP/*",
          "$DOWNLOAD/*"
        ]
      },
      "window": {
        "all": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "blob-downloader",
        "width": 1200,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
