{
  "name": "blob-downloader",
  "private": true,
  "version": "0.4.0",
  "description": "Convert .m3u8 file to video",
  "license": "MIT",
  "scripts": {
    "prebuild": "node ./scripts/move-binaries.js",
    "build": "stencil build",
    "prestart": "node ./scripts/move-binaries.js",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "tauri": "tauri"
  },
  "dependencies": {
    "@ionic/core": "6.2.1",
    "@tauri-apps/api": "^1.0.2",
    "execa": "^6.1.0",
    "nexus-http": "~2.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@stencil/core": "2.17.3",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^2.0.0",
    "@tauri-apps/cli": "^1.0.5",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
